{
  "expo": {
    "name": "Dinner Generator",
    "slug": "Dinner-Generator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/resources/images/icon.png",
    "scheme": "dinnerGenerator",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/resources/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/resources/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.dinnergenerator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/resources/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
